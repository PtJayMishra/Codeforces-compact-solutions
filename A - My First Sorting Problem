import java.util.Scanner;

public class Myfirstsorting {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        for (int i = 0; i <t ; i++) {
            int x = in.nextInt();
            int y = in.nextInt();
            if (y>x){
                System.out.println(x +" "+ y);
            }else {
                System.out.println(y+" "+ x);
            }
        }
    }
}
